#예제1
hap=0
start=int(input("시작값을 입력하세요"))
stop=int(input("종료값을 입력하세요"))
step=int(input("증가값을 입력하세요"))

for i in range(start, stop+1, step):
  hap=hap+i
print("%d에서 %d까지 %d씩 증가시킨 값의 합계: %d"%(start, stop,step,hap))

#예제2
sum=0
count=0

for i in range(2,10001,2):
  sum=sum+i
  count=count+1
print(sum)
print(count)
print(int(sum/count))

#예제3
raw=input("문자열을 입력하세요")
str1=""

for i in raw:
  if i != "-":
    str1=str1+i
print(str1)
